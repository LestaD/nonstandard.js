{
  "name": "nonstandard",
  "version": "1.1.0",
  "description": "Non standard JavaScript features",
  "main": "src/lib.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -r should -R spec",
    "coverage": "NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- -r should -R spec",
    "cov:dev": "NODE_ENV=test npm run coverage && open ./coverage/lcov-report/index.html",
    "toc": "doctoc --title \"**Contents**\" --github README.md ",
    "changelog": "my-changelog --group true --filter-release true"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "directories": {
    "lib": "./src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LestaD/nonstandard.js.git"
  },
  "keywords": [
    "non",
    "standard",
    "polyfill",
    "feature",
    "ecma"
  ],
  "author": "LestaD <i.am@lestad.net> (Sergey Sova)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LestaD/nonstandard.js/issues"
  },
  "homepage": "https://github.com/LestaD/nonstandard.js#readme",
  "devDependencies": {
    "mocha": "^2.3.4",
    "should": "^8.1.1",
    "should-sinon": "0.0.5",
    "sinon": "^1.17.2"
  }
}
